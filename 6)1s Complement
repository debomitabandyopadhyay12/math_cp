string onesComplement(string s,int n){
        string ans="";
        for(int i=0;i<n;i++)
        {
            if(s[i]=='0')
            {
                ans=ans+'1';
            }
            else if(s[i]=='1')
            {
                ans=ans+'0';
            }
        }
        return ans;
    }

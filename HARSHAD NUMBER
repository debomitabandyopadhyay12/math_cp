#include<bits/stdc++.h>
using namespace std;
int main()
{   
    int num=378;
	string s = to_string(num);
	int sum = 0;
	for (int i = 0; i < s.length(); i++) {
		sum += s[i] - '0';
	}
	if (num % sum == 0) {
		cout << "YES it is Harshad Number" << "\n";
	}
	else {
		cout << "NO it is not a Harshad Number" << "\n";
	}

}
